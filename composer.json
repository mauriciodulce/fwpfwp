{
    "name": "mauriciodulce/fwpfwp",
    "description": "The WordPlate framework",
    "keywords": [
        "boilerplate",
        "framework",
        "wordplate",
        "wordpress"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mauricio Dulce",
            "email": "hola@mauriciodulce.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "composer/installers": "^1.9",
        "johnpbloch/wordpress-core-installer": "^2.0",
        "johnpbloch/wordpress-core": "^5.5",
        "roots/bedrock-autoloader": "^1.0",
        "roots/wp-password-bcrypt": "^1.0",
        "symfony/http-foundation": "^5.0",
        "symfony/routing": "^5.0",
        "symfony/var-dumper": "^5.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "preferred-install": "dist"
    },
    "autoload": {
        "psr-4": {
            "MauricioDulce\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MauricioDulce\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
